{
  "$schema": "http://json-schema.org/schema#",
  "title": "Protocol Schema",
  "description": "JSON-schema for a Protocol.",
  "type": "object",
  "properties": {
    "@id": { "type": "string", "format": "uri" },
    "name": { "type": "string" },
    "description": { "type": "string" },
    "protocol_vocabulary": { "type": "string" },
    "protocol_term": { "type": "string" },
    "qualitative_factors": {
      "type": "array",
      "items": { "$ref": "qualitative_factor.json#" } },
    "quantitative_factors": {
      "type": "array",
      "items": { "$ref": "quantitative_factor.json#"} }
  },
  "additionalProperties": false
}
