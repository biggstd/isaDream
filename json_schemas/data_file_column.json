{
  "$schema": "http://json-schema.org/schema#",
  "title": "Data Column Label Schema",
  "description": "JSON-schema denoting a column of data in a given .csv file.",
  "type": "object",
  "properties": {
    "@id": { "type": "string", "format": "uri" },
    "column_index": { "type": "integer" },
    "qualifier_label": { "type": "string" },
    "column_unit": { "type": "string" },
    "column_sample": {
      "sample_id": { "type": "string" },
      "sample_factor_label": { "type": "string" },
      "sample_unit": { "type": "string" },
      "sample_treatment_type": { "type": "string" },
      "sample_treatment_unit": { "type": "string" },
      "sample_treatment_label": { "type": "string" }
    }
  },
  "additionalProperties": false
}
