{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "title": "Publication Schema",
  "properties": {
    "$id": { "type": "string", "format": "uri" },
    "pubMedID" : { "type" : "string" },
    "doi" : { "type" : "string"},
    "authorList" : { "type" : "string" },
    "comments" : {
      "type" : "array",
      "items": { "$ref": "comment_schema.json" }
    }
  }
}